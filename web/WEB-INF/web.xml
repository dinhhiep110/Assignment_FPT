        <servlet-name>DaNangDragonController</servlet-name>
        <url-pattern>/vba/dnd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HCMCityWingsController</servlet-name>
        <url-pattern>/vba/hcm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CanThoCatFishController</servlet-name>
        <url-pattern>/vba/ctc</url-pattern>
    </servlet-mapping>
