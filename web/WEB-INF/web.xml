        <servlet-name>HaNoiBuffaloesController</servlet-name>
        <url-pattern>/vba/hnb</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
